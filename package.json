{
  "name": "MobileCoachClient",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "precommit": "npm run git-hook",
    "prepush": "npm run git-hook",
    "git-hook": "npm run lint -s && npm run test -s",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix; exit 0"
  },
  "dependencies": {
    "apisauce": "^0.14.1",
    "babel-polyfill": "^6.26.0",
    "crypto-js": "^3.1.9-1",
    "deepstream.io-client-js": "^2.3.0",
    "format-json": "^1.0.3",
    "fuse.js": "^3.2.0",
    "immutability-helper": "^2.6.4",
    "lodash": "^4.17.2",
    "lottie-react-native": "^2.3.1",
    "pdfmake": "^0.1.35",
    "prop-types": "^15.6.0",
    "querystringify": "1.0.0",
    "ramda": "^0.25.0",
    "react": "16.2.0",
    "react-native": "0.52.0",
    "react-native-action-button": "^2.8.1",
    "react-native-animatable": "^1.2.4",
    "react-native-axios": "^0.17.1",
    "react-native-button": "2.2.0",
    "react-native-cached-image": "^1.4.3",
    "react-native-cardview": "^1.1.5",
    "react-native-collapsible": "^0.10.0",
    "react-native-config": "^0.11.5",
    "react-native-datepicker": "https://github.com/afiller/react-native-datepicker.git#ios-locale-fix",
    "react-native-device-info": "0.13.0",
    "react-native-drawer": "^2.5.0",
    "react-native-easy-toast": "^1.0.9",
    "react-native-elements": "^0.19.0",
    "react-native-fabric": "^0.5.1",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-file-viewer": "^1.0.1",
    "react-native-fit-image": "^1.5.4",
    "react-native-fs": "^2.9.11",
    "react-native-gifted-chat": "^0.3.0",
    "react-native-global-props": "^1.1.3",
    "react-native-html-parser": "^0.0.5",
    "react-native-htmlview": "^0.12.1",
    "react-native-i18n": "2.0.10",
    "react-native-iphone-x-helper": "^1.0.2",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-material-dropdown": "^0.6.1",
    "react-native-navbar": "^2.1.0",
    "react-native-piwik": "^0.0.8",
    "react-native-push-notification": "^3.0.1",
    "react-native-searchbar": "^1.13.0",
    "react-native-side-menu": "^1.1.3",
    "react-native-simple-radio-button": "^2.7.0",
    "react-native-simple-store": "^1.3.0",
    "react-native-slider": "^0.11.0",
    "react-native-spinkit": "^1.1.1",
    "react-native-splash-screen": "^3.0.6",
    "react-native-tab-view": "^0.0.74",
    "react-native-vector-icons": "^4.5.0",
    "react-navigation": "1.0.0-beta.27",
    "react-redux": "^5.0.6",
    "redux": "^3.6.0",
    "redux-persist": "^4.10.2",
    "redux-saga": "^0.16.0",
    "reduxsauce": "0.6.0",
    "reselect": "^3.0.1",
    "seamless-immutable": "^7.0.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "babel-jest": "22.0.4",
    "babel-plugin-ignite-ignore-reactotron": "^0.3.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react-native": "4.0.0",
    "enzyme": "^3.3.0",
    "husky": "^0.14.3",
    "jest": "22.0.4",
    "jest-cli": "^22.0.4",
    "mockery": "^2.0.0",
    "react-addons-test-utils": "~15.6.2",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.0.0",
    "reactotron-react-native": "^1.12.3",
    "reactotron-redux": "^1.12.2",
    "reactotron-redux-saga": "^1.12.2",
    "snazzy": "^7.0.0",
    "standard": "10.0.3"
  },
  "rnpm": {
    "assets": [
      "./Web/fonts-also-app/"
    ]
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "**/Tests/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "Tests/Setup.js",
      "Tests/AssetTransformer.js"
    ],
    "setupFiles": [
      "./Tests/Setup"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/Tests/AssetTransformer.js",
      "\\.(css|less)$": "<rootDir>/Tests/AssetTransformer.js"
    }
  },
  "config": {},
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  }
}
